{
  "schema": "https://www.raycast.com/schemas/extension.json",
  "name": "bonsai-notes",
  "title": "Bonsai Notes",
  "description": "Raycast exrtension to interact with Bonsai Notes",
  "icon": "command-icon.png",
  "author": "ManiacalRobot",
  "license": "MIT",
  "commands": [
    {
      "name": "post_note",
      "title": "Post note to Bonsai ...",
      "description": "Post a new Note to a Bonsai Book",
      "mode": "view"
    },
    {
      "name": "post_stats",
      "title": "Set daily Stat ...",
      "description": "set your daily stats",
      "mode": "view"
    }
  ],
  "preferences": [
    {
      "name": "api-key",
      "description": "Your API Key generated on Bonsai Notes",
      "type": "password",
      "required": true,
      "title": "API Key"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.45.1",
    "axios": "^1.2.2"
  },
  "devDependencies": {
    "@types/node": "18.8.3",
    "@types/react": "18.0.9",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "prettier": "^2.5.1",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "ray publish"
  }
}
